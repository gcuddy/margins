{
	"name": "@margins/annotator",
	"private": true,
	"sideEffects": false,
	"version": "0.0.0",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./src/index.ts",
			"require": "./src/index.ts",
			"svelte": "./src/index.ts",
			"types": "./src/index.ts",
			"default": "./src/index.ts",
			"node": "./src/index.ts"
		},
		"./schema": {
			"import": "./src/schema.ts",
			"require": "./src/schema.ts",
			"svelte": "./src/schema.ts",
			"types": "./src/schema.ts",
			"default": "./src/schema.ts",
			"node": "./src/schema.ts"
		},
		"./dom": {
			"import": "./src/dom/index.ts",
			"require": "./src/dom/index.ts",
			"svelte": "./src/dom/index.ts",
			"types": "./src/dom/index.ts",
			"default": "./src/dom/index.ts",
			"node": "./src/dom/index.ts"
		}
	},
	"scripts": {
		"lint": "eslint . --ext .ts,.js,.tsx,.jsx",
		"lint:fix": "eslint . --ext .ts,.js,.tsx,.jsx --fix",
		"lint:report": "eslint . --format json --output-file ../../lint-results/app-store.json"
	},
	"dependencies": {
		"@floating-ui/core": "^1.6.0",
		"@margins/lib": "workspace:*",
		"lucide-svelte": "^0.441.0",
		"svelte-floating-ui": "^1.5.8",
		"svelte-writable-derived": "^3.1.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@margins/config": "workspace:*",
		"@margins/features": "workspace:*",
		"@margins/tsconfig": "workspace:*",
		"eslint": "~8.57.1",
		"type-fest": "^4.26.1",
		"typescript": "^5.2.2",
		"vitest": "^2.1.1"
	},
	"type": "module"
}
