{
	"name": "@margins/ui",
	"private": true,
	"sideEffects": false,
	"version": "0.0.0",
	"main": "./index.ts",
	"exports": {
		".": {
			"import": "./index.ts",
			"svelte": "./index.ts",
			"types": "./index.ts"
		},
		"./styles/colors.css": "./styles/colors.css",
		"./styles/style.css": "./styles/style.css"
	},
	"types": "./index.ts",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"lint": "eslint .",
		"type-check": "tsc --pretty --noEmit",
		"type-check:ci": "tsc-absolute --pretty --noEmit",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"lint:fix": "eslint . --fix",
		"lint:report": "eslint . --format json --output-file ../../lint-results/ui.json"
	},
	"svelte": "./index.ts",
	"dependencies": {
		"@margins/lib": "workspace:*",
		"@melt-ui/svelte": "^0.83.0",
		"@radix-ui/colors": "^3.0.0",
		"@storybook/addon-svelte-csf": "^4.1.2",
		"@storybook/blocks": "^8.0.4",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"autoprefixer": "^10.4.19",
		"bits-ui": "^0.21.2",
		"cmdk-sv": "^0.0.18",
		"formsnap": "^1.0.1",
		"lucide-svelte": "^0.441.0",
		"postcss": "^8.4.38",
		"react": "^18.2.0",
		"svelte-local-storage-store": "^0.6.4",
		"svelte-radix": "^1.1.1",
		"svelte-tiptap": "^1.1.2",
		"sveltekit-superforms": "^2.19.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.2"
	},
	"devDependencies": {
		"@margins/config": "workspace:*",
		"@margins/tsconfig": "workspace:*",
		"@melt-ui/pp": "^0.3.2",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/svelte": "^5.2.1",
		"svelte": "^5.0.0-next.242",
		"typescript": "^5.2.2"
	}
}
