{
	"name": "@margins/ui",
	"private": true,
	"sideEffects": false,
	"version": "0.0.0",
	"main": "./index.ts",
	"exports": {
		".": "./index.ts",
		"./styles/colors.css": "./styles/colors.css",
		"./styles/style.css": "./styles/style.css"
	},
	"types": "./index.ts",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"lint": "eslint .",
		"type-check": "tsc --pretty --noEmit",
