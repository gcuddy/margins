		"bits-ui": "^0.20.0",
		"lucide-svelte": "^0.363.0",
		"postcss": "^8.4.38",
		"react": "^18.2.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.2"
	},
	"devDependencies": {
		"@margins/config": "workspace:*",
		"@margins/tsconfig": "workspace:*",
		"@melt-ui/pp": "^0.1.2",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/svelte": "^4.1.0",
		"typescript": "^5.2.2"
	}
