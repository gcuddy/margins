{
	"extends": "@margins/tsconfig/base.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"resolveJsonModule": true,
		"noUncheckedIndexedAccess": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		// "module": "ESNext",
		// "moduleResolution": "Bundler",
		"types": ["vitest/globals"]
	},
	"include": ["**/*.ts"]
}
